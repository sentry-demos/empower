env: standard
service: ${REACT_APP_ENGINE_SERVICE}
runtime: nodejs20

handlers:
  - url: /static
    static_dir: build/static
  - url: /(.*\.(json|ico|js|png))$
    static_files: build/\1
    upload: build/.*\.(json|ico|js|png)$
  - url: .*
    static_files: build/index.html
    upload: build/index.html
    http_headers:
      Document-Policy: js-profiling
